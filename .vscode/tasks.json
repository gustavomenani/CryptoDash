{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"type": "shell",
			"command": "npx tsc --noEmit && npx vite build",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Type Check",
			"type": "shell",
			"command": "npx tsc --noEmit"
		},
		{
			"label": "Start Server",
			"type": "shell",
			"command": "node server.js"
		},
		{
			"label": "Dev",
			"type": "shell",
			"command": "npx vite"
		},
		{
			"label": "Test",
			"type": "shell",
			"command": "npx jest"
		}
	]
}